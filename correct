// Лабораторна робота No 3.4
// Розгалуження, задане плоскою фігурою.
// Варіант 4
#include <iostream>
using namespace std;
int main()
{
	double x;
	double y;
	double R;

	cout << "R = "; cin >> R;
	cout << "x = "; cin >> x;
	cout << "y = "; cin >> y;

	if (
		(y >= 0 && y <= R && x >= -R && x <= R
			&& (pow(x, 2) + pow(y, 2) - R >= 0)) ||
		(y >= (-1) * R && y <= 0 && x >= -R && x <= 0 && y <= R * sqrt(2))
		)
		cout << "yes" << endl;
	else
		cout << "no" << endl;
	cin.get();
	return 0;
}
